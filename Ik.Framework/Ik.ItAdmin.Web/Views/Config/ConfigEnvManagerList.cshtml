
@{
    ViewBag.Title = "配置环境管理";
}

<table width="100%" height="100%" border="0" cellspacing="0" cellpadding="0" class="listpage">
    <tr height="24px">
        <td class="title">配置环境管理<span class="arrowdown" onclick="foldSearchPanel(this);"></span></td>
    </tr>
    <tr height="30px">
        <td class="search">
            <div style="width: 200px; float: left;height:40px;">
                <a href="#" class="wiscobutton" onclick="showWiscoDialog('@Url.Content("~/Config/CfgEnvAdd")', 400, 250,null,null,null,RetrunValue);"><span class="left"></span><span class="center">新增</span><span class="right"></span></a>
            </div>
            <div style="clear:both;"></div>
        </td>
    </tr>
    <tr>
        <td class="grid">
            <div id="wiscogrid1" class="wiscogrid">
                @Html.Action("GetCfgEnvList");
            </div>
        </td>
    </tr>
    <tr height="30px" id="pager1">
        <td>
            @Html.Partial("_HtmlPagerEmpty")
        </td>
    </tr>
</table>
<script>
    function RetrunValue(argu) {
        if (argu == "success") {
            $.ajax({
                url: "/Config/GetCfgEnvList",
                type: "get",
                cache: false
            }).done(function (html) {
                $("#wiscogrid1").html(html);
            });

        }
    };
</script>